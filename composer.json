{
    "name": "lukeoliff/zpg-rtf-php",
    "description": "Package for ZPG Real Time Feed integration",
    "type": "library",
    "require": {
        "php": "~7",
        "guzzlehttp/guzzle": "^6.3",
        "php-di/php-di": "^5.4"
    },
    "autoload": {
        "psr-4": {
            "ZpgRtf\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ZpgRtf\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^6.2",
        "symfony/var-dumper": "~3"
    },
    "license": "MIT",
